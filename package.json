{
  "name": "mastermind",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "dev:open": "vite --host --open",
    "build": "vite build",
    "package": "vite package",
    "preview": "vite preview",
    "deploy:dev": "deploy.sh && vite --host",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "format": "prettier --ignore-path .gitignore --write '**/*.{css,cjs,html,js,json,svelte,svg}'",
    "format:check": "prettier --ignore-path ../.gitignore --check '**/*.{css,cjs,html,js,json,svelte,svg}'",
    "lint": "eslint '**/*.{cjs,html,js,svelte}'",
    "lint:fix": "eslint --fix '**/*.{cjs,html,js,svelte}'",
    "rollup": "rollup -c rollup.config.js",
    "rollup:watch": "rollup -c rollup.config.js -w"
  },
  "eslintIgnore": [
    ".svelte-kit",
    "build",
    "dist",
    "node_modules"
  ],
  "dependencies": {
    "svelte-lib": "file:../svelte-lib",
    "vite": "^4.4.8"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-image": "^3.0.2",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-url": "^8.0.1",
    "@shopify/prettier-plugin-liquid": "^1.2.1",
    "postcss-import": "^15.1.0",
    "rollup-plugin-ignore": "^1.0.10",
    "rollup-plugin-url": "^3.0.1",
    "rollup": "^3.27.2",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svelte": "^7.1.6",
    "rollup-plugin-svg": "^2.0.0",
  }
}
